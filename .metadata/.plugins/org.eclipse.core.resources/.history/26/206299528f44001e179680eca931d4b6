package a35Access_Modifiers;

public class cpublic_modifier {

	public static void main(String[] args) {

		bpublic_modifier bpm = new bpublic_modifier();
		System.out.println(bpm.rollNo);
		bpm.printRollNumber();
		
		/*
		 * public modifier petamu anuko same class lo call chaiyalantea object create chaiyali
		   and same package lo call chaiyalantea object create chaiyali.
		 * public modifier ni verea package lo call chaiyali antea object create chaiyali.
		 * public modifier ni sub class(extends main class) lo call chaiyali antea object create chaiyali.
		 */
		
		
	}

}
