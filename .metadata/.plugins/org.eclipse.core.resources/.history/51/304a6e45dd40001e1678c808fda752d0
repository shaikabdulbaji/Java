package a29foreach_loop;

public class aSample {
	
	/*
	 * Syntax for for-each loop :
	   for(dataType varName : array (or) collection)
	   {
	   		//statements
	   }
	 */
	
	public static void main(String[] args) {
		
		int[] arr = {1,2,4,6,8,0};

		for(int i=0; i<arr.length; i++)
		{
			System.out.println(arr[i]);
		}
		
	}

}
