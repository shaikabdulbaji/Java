package a21Decision_making_Statements;

/*
 * if and elseif both are condition based conditional statements.
 * switch is a value based conditional statements.
 * Syntax : 
   switch(key){
   		Case value;
	   		//statements
	   		break;
	   		
	   	default;
	   		//statements
	   		break;
   }
 */

public class switch_ {
	
	public static void main(String[] args) {
		
		int i = 5;
		switch() {
		
		}
		
		/*
		 * 
		 */
		
		
	}

}
