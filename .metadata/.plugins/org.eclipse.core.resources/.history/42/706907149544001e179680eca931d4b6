package a35Access_Modifiers;

import a35Access_Modifiers.bpublic_modifier;
import a35Access_Modifiers.dprivate_modifier;

public class edefault_modifier1 {

	public static void main(String[] args) {

		edefault_modifier bpm = new edefault_modifier();
		System.out.println(bpm.rollNo);
		bpm.printRollNumber();
		
		/*
		 * private modifier untea dani out side class or inside package kuda access chaiyaleamu.
		 * vaka veala main class lo constructor ni private di public cheasina ekada object create
		   cheasukovachu emo kani variables and methods access chaiyaleamu.
		 */
		
	}

}
