package a59Inner_classes_Part2;

/*
 * class inside one block is called local class. 
 */

public class aLocal_class {
	
	public static void main(String[] args) {
		
		int id = 100;
		double salary = 10000;
		abstract class A {
			
		}
		
		final class B {
			
		}
		
		class f {
			
		}
	}
	
	public void show() {
		System.out.println("Show Method is Called");
	}
	
	public aLocal_class() { //constructor
		
		class c {
			
		}
	}
	
	{
		class d {
			
		}
	}
	
	static {
		class e {
			
		}
	}

}
