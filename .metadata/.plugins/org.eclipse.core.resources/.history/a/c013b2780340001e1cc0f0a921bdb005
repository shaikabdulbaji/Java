package a25Looping_Statements;

public class cdo_while {
	
	public static void main(String[] args) {
		
		int i = 0;
		
		do {
			System.out.println(i);
			i++;
		}
		while(i<10);
		
		/*
		 * aa while lo vaka condition undi kada i<10 ani adi 10 untea 0 to 9 print aavutai
		   adea 0 untea 0 ani print cheasidi, do-while lo fail ina vaka sari print cheasidi
		   ndukantea statement tarvatea condition undi anduku. 
		 */
	}

}
