package a35Access_Modifiers;

public class edefault_modifier2 {

	public static void main(String[] args) {

		edefault_modifier bpm = new edefault_modifier();
		System.out.println(bpm.rollNo);
		bpm.printRollNumber();
		
		/*
		 * default modifier untea dani inside class or inside package kuda access chaiyaleamu.
		 * default modifer aneadi classes ki and variables ki and methods ki use cheasukovachu.
		 * andukea dini package-private ani kuda antamu.
		 */
		
	}

}
