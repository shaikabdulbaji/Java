package a22Constructor;

public class eMemory_allocation_with_constructors {
	
	public int i;
	
	public static void main(String[] args) {
		
		eMemory_allocation_with_constructors ec = new eMemory_allocation_with_constructors();
		//ekada manam i ki a value evaleadu apudu manam i ni print chestea default value for int is "0" print iedi.
		System.out.println(ec.i);
		
	}

}
