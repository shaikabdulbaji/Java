package a35Access_Modifiers;

public class cpublic_modifier {

	public static void main(String[] args) {

		bpublic_modifier bpm = new bpublic_modifier();
		System.out.println(bpm.rollNo);
		bpm.printRollNumber();
		
		/*
		 * public aneadi class ki and variable ki and method ki use cheasukovachu.
		 * public modifier petamu anuko same class lo call chaiyalantea object create chaiyali
		   and same package lo call chaiyalantea object create chaiyali.
		 * public modifier ni verea package lo call chaiyali antea object create chaiyali.
		 * public modifier ni sub class(extends main class) lo call chaiyali antea direct ga call
		   chaiyochu vaka method create cheasi and aa method ni main class lo call chestea saripotadi.
		   
		 * public modifier aneadi inside package and outside package and sub class e muditlatho 
		   access cheasukovachu.
		 */
		
		
	}

}
