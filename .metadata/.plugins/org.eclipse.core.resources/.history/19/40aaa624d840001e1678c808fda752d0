package a28Array_Multi_DimensionalArray;

public class bMulti_Dimensional_Array {

	public static void main(String[] args) {
		
		int[][] arr = new int[3][2];

		arr[0][0] = 1;
		arr[0][1] = 2;
		
		arr[1][0] = 3;
		arr[1][1] = 4;
		
		arr[2][0] = 5;
		arr[2][1] = 6;
		
		//System.out.println(arr[0][0]);	
		
		for(int i=0; i<arr.length; i++)
		{
			System.out.println(arr);
		}
		
	}

}
