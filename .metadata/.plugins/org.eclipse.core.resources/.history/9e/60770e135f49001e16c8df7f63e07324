package a59Inner_classes_Part2;

public class cLocal_class2 {
	
	public static void main(String[] args) {
		
		int id = 100;
		double salary = 10000;
		abstract class A {
			
		}
		
		final class B {
			
		}
		
		class f {
			
		}
	}
	
	public void show() {
		System.out.println("Show Method is Called");
	}
	
	public cLocal_class2() { //constructor
		
		class c {
			
		}
	}
	
	{ //non-static block
		class d {
			
		}
	}
	
	static { //static block
		class e {
			
		}
	}

}
