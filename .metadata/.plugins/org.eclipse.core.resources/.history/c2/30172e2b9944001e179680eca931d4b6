package a35Access_Modifiers;

public class fprotected_modifier1 {

	public static void main(String[] args) {

		fprotected_modifier bpm = new fprotected_modifier();
		System.out.println(bpm.rollNo);
		bpm.printRollNumber();
		
		/*
		 * protected modifier untea dani inside class or inside package kuda access chaiyavachu.
		 * protected modifer aneadi variables ki and methods ki use cheasukovachu.
		 * but protected aneadi classes ki use chaiyaleamu.
		 * 
		 */
		
	}

}
