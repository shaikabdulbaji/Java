package a59Inner_classes_Part2;

public class eAnonymous_class {
	
	public static void main(String[] args) {
		
		int id = 100;
		int salary = 10000;

		
	}
	
	public void show() {
		System.out.println("Show Method is Called");
	}
	
	class innerclass1 {
		
		public void show() {
			System.out.println("Show Method is Called");
		}
		
		public void print() {
			System.out.println("Show Method is Called");
		}
	}
}
