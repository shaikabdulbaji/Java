package a66StingBuffer_and_StringBuilder;

public class cStringBuffer {

	public static void main(String[] args) {

		StringBuffer sb1 = new StringBuffer("abdul");
		StringBuffer sb2 = new StringBuffer("abdul");
		
		System.out.println(sb1==sb2);
		
		sb1 = sb1.append("shaik");
		System.out.println(sb1==sb2);
		
		String s3 = "shaik baji";
		System.out.println(s1==s3);
		
		
	}

}
