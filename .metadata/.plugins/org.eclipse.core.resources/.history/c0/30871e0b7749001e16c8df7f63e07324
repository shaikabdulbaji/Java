package a62Lambda_expressions;

public class cBy_using_LambdaExpressions {
	
	public static void main(String[] args) {
		
		//Lambda expression with anonymous functions
		bike c1 = () -> {
			System.out.println("Driving tata");
		  };
		}
		c1.ride();
	}
}

@FunctionalInterface
interface bike {
	public void ride();
}